<p id="notice"><%= notice %></p>

<h1>Listing Webhooks</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Recipient</th>
      <th>Domain</th>
      <th>Campaign</th>
      <th>User agent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webhooks.each do |webhook| %>
      <tr>
        <td><%= webhook.event %></td>
        <td><%= webhook.recipient %></td>
        <td><%= webhook.domain %></td>
        <td><%= webhook.campaign_id %></td>
        <td><%= webhook.user_agent %></td>
        <td><%= link_to 'Destroy', webhook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
